  "scripts": {
    "linter": "eslint --ext .js,.jsx .",
    "linter:fix": "eslint --ext .js,.jsx . --fix",
    "install:link-deps": "npm install && npm link @adobe/cq-spa-page-model-manager && npm link @adobe/cq-spa-component-mapping",
    "test": "BABEL_ENV=test npm run build && karma start --browsers=ChromeHeadless --single-run=true",
    "test-debug": "BABEL_ENV=test npm run build && karma start --browsers=Chrome_with_debugging",
    "build": "npm run linter && babel src --out-dir dist",
    "docs": "jsdoc -c ./jsdoc_conf.json && doxdox \"src/**/*.+(js|jsx)\" --layout templates/DOCUMENTATION.hbs --output DOCUMENTATION.md",
    "readme": "node_modules/markdown-include/bin/cli.js markdown-include.config.json",
    "docs-readme": "npm run docs && npm run readme",
    "preversion": "npm run build && npm run test && npm run docs-readme",
    "version": "npm run docs-readme && git add -A",
    "postversion": "git push && git push --tags"
  },
  "author": "Adobe Systems Inc.",
  "license": "Apache-2.0",
