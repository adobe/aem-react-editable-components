    "karma-webpack": "^3.0.0",
    "mocha": "^5.1.0",
    "npm-auto-version": "^1.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "sinon": "^4.5.0",
