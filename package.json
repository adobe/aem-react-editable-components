{
  "name": "@adobe/cq-react-editable-components",
  "version": "1.0.2",
  "auto-changelog": {
    "template": "templates/CHANGELOG.hbs"
  },
  "bugs": {
    "url": "https://www.adobe.com/go/aem6_4_support_en"
  },
  "engines": {
    "npm": ">=5.5.1 <6.0.0"
  },
  "files": [
    "dist/"
  ],
  "homepage": "https://www.adobe.com/go/aem6_4_docs_spa_en",
  "keywords": [
    "the official",
    "adobe",
    "aem",
    "spa editor",
    "experience manager",
    "marketing cloud",
    "cq",
    "in-context editing",
    "in-place editing",
    "spa authoring",
    "reactjs",
    "pwa"
  ],
  "main": "dist/cq-react-editable-components.js",
  "scripts": {
    "linter": "eslint --ext .js,.jsx .",
    "linter:fix": "eslint --ext .js,.jsx . --fix",
    "install:link-deps": "npm install && npm link @adobe/cq-spa-page-model-manager && npm link @adobe/cq-spa-component-mapping",
    "test": "npm run build && cross-env NODE_ENV=test karma start --browsers=ChromeHeadless --single-run=true",
    "test:debug": "npm run build && cross-env NODE_ENV=test karma start --browsers=Chrome_with_debugging",
    "build": "npm run linter && webpack",
    "build:production": "npm run linter && webpack --mode=production",
    "docs": "jsdoc -c ./jsdoc_conf.json && doxdox \"src/**/*.+(js|jsx)\" --layout templates/DOCUMENTATION.hbs --output DOCUMENTATION.md",
    "readme": "node node_modules/markdown-include/bin/cli.js markdown-include.config.json",
    "docs-readme": "npm run docs && npm run readme",
    "preversion": "npm run test && npm run build:production && npm run docs-readme",
    "version": "npm run docs-readme && git add DOCUMENTATION.md README.md package.json",
    "postversion": "git push origin && git push origin --tags",
    "tmprelease": "node release.js"
  },
  "author": "Adobe Systems Inc.",
  "license": "Apache-2.0",
  "publishConfig": {
    "registry": "https://artifactory.corp.adobe.com/artifactory/api/npm/npm-cq-spa-release-local"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2015,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    }
  },
  "peerDependencies": {
    "@adobe/cq-spa-component-mapping": "~1.0.3",
    "@adobe/cq-spa-page-model-manager": "~1.0.0",
    "react-fast-compare": "^2.0.2"
  },
  "optionalDependencies": {
    "custom-event-polyfill": "^1.0.6",
    "babel-polyfill": "^6.26.0"
  },
  "devDependencies": {
    "@adobe/cq-spa-component-mapping": "~0.0.17-beta.3",
    "@adobe/cq-spa-page-model-manager": "~0.0.25-beta.12",
    "ajv": "^5.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "7.2.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.19",
    "clone": "^2.1.2",
    "cross-env": "^5.2.0",
    "cross-fetch": "^2.2.2",
    "doxdox": "^2.0.3",
    "eslint": "4.3.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-react": "^7.11.1",
    "executive": "1.6.3",
    "inject-loader": "2.0.1",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdoc": "^3.5.5",
    "json-loader": "^0.5.7",
    "karma": "^0.13.22",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-sinon": "^1.0.5",
    "karma-sinon-chai": "^1.3.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "markdown-include": "^0.4.3",
    "mocha": "^5.1.0",
    "npm-auto-version": "^1.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-fast-compare": "^2.0.2",
    "react-test-renderer": "^16.2.0",
    "readline-sync": "^1.4.9",
    "sinon": "^4.5.0",
    "sinon-chai": "^2.14.0",
    "webpack": "^4.17.1",
