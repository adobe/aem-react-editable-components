{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": ["es2017", "dom"],
    "declaration": true,
    "allowJs": true,
    "emitDeclarationOnly": true,
    "strict": false,
    "alwaysStrict": false,
    "allowUnreachableCode": true,
    "types": ["node"],
    "noImplicitThis": false,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "jsx": "react",
    "outFile": "./dist/cq-react-editable-components.d.ts"
  },
  "include": ["src"]
}