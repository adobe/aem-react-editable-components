{
  "recurseDepth": 10,
  "source": {
    "include": ["./index.js", "./src/"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "readme": "./README.md",
    "template": "templates/default",
    "encoding": "utf8",
    "destination": "./out/",
    "recurse": true
  }
}